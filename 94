def inorderHelper(allVal, node):
    if node == None:
        return
    inorderHelper(allVal, node.left)
    allVal.append(node.val)
    inorderHelper(allVal, node.right)
class Solution(object):
    def inorderTraversal(self, root):
        allVal = []
        inorderHelper(allVal, root)
        return allVal
